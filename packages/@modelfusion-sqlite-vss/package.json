{
  "name": "@modelfusion/sqlite-vss",
  "description": "SQLite VSS Vector DB for ModelFusion",
  "version": "0.3.0",
  "author": "Lars Grammel",
  "license": "MIT",
  "keywords": [
    "pinecone",
    "modelfusion",
    "vectordb"
  ],
  "homepage": "https://modelfusion.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lgrammel/modelfusion"
  },
  "bugs": {
    "url": "https://github.com/lgrammel/modelfusion/issues"
  },
  "type": "module",
  "sideEffects": false,
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "files": [
    "./*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "clean": "rimraf build dist .turbo node_modules",
    "build": "pnpm build:esm && pnpm build:cjs && pnpm build:copy-files",
    "build:esm": "tsc --outDir dist/",
    "build:cjs": "tsc --outDir build/cjs/ -p tsconfig.cjs.json && node bin/prepare-cjs.js",
    "build:copy-files": "copyfiles --flat package.json README.md ../../LICENSE dist",
    "dist": "pnpm clean && pnpm lint && pnpm test && pnpm build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.6",
    "@types/node": "20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "9.1.0",
    "modelfusion": "0.119.0",
    "sqlite-vss": "^0.1.2",
    "better-sqlite3": "^9.0.0"
  },
  "peerDependencies": {
    "modelfusion": ">=0.72.0 <1.0.0-0",
    "sqlite-vss": "^0.1.2",
    "better-sqlite3": "^9.0.0"
  }
}
